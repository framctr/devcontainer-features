{
    "id": "openstack-client",
    "version": "1.0.0",
    "name": "OpenStack Client (from pipx)",
    "documentationURL": "https://github.com/framctr/devcontainer-features/tree/main/src/openstack-client",
    "description": "Install an up-to-date version of OpenStack client with pipx.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter an OpenStack client version."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}