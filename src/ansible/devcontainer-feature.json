{
    "id": "ansible",
    "version": "1.0.0",
    "name": "ansible",
    "documentationURL": "https://github.com/framctr/devcontainer-features/tree/main/src/ansible",
    "description": "Install ansible.",
    "options": {
        "package": {
            "type": "string",
            "proposals": [
                "ansible",
                "ansible-core"
            ],
            "default": "ansible-core",
            "description": "Select the kind of package to install."
        },
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter an ansible version."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/python"
    ]
}