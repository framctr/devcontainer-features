{
    "id": "krew",
    "version": "0.0.1",
    "name": "krew",
    "documentationURL": "https://github.com/framctr/devcontainer-features/tree/main/src/krew",
    "description": "Install krew.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter a krew version."
        }
    },
    "containerEnv": {
        "PATH": "${KREW_ROOT:-$HOME/.krew}/bin:${PATH}"
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/kubectl-helm-minikube",
        "ghcr.io/devcontainers/features/common-utils"
    ]
}